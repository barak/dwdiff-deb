#, c-format
msgid "Single backslash at end of %s argument\n"
msgstr "Losse backslash aan het einde van de %s argument\n"

#. TRANSLATORS:
#. The %s argument is a long option name without preceding dashes.
#: option.c:134 option.c:141
#, c-format
msgid "Invalid hexadecimal escape sequence in %s argument\n"
msgstr "Ongeldige hexadecimale escapecode in de %s argument\n"

#. TRANSLATORS:
#. The %c argument will be either 'u' or 'U'. The %s argument is a long
#. option name without preceding dashes.
#: option.c:169 option.c:175
#, c-format
msgid "Too short \\%c escape in %s argument\n"
msgstr "Te korte \\%c escapecode in %s argument\n"

#. TRANSLATORS:
#. The %s argument is a long option name without preceding dashes.
#: option.c:186
#, c-format
msgid "\\U escape out of range in %s argument\n"
msgstr "\\U escapecode buiten geldig bereik in %s argument\n"

#. TRANSLATORS:
#. The %s argument is a long option name without preceding dashes.
#: option.c:191
#, c-format
msgid "\\%c escapes for surrogate codepoints are not allowed in %s argument\n"
msgstr ""
"\\%c escapecodes voor surrogaat codepoints zijn niet toegestaan in %s "
"argument\n"

#: option.c:307 option.c:315 option.c:326
msgid "Whitespace and delimiter sets overlap\n"
msgstr "Witruimte en afbakeningskarakters verzamelingen overlappen\n"

#: option.c:332
msgid "-h, --help                             Print this help message\n"
msgstr "-h, --help                             Toon dit help bericht\n"

#: option.c:333
msgid ""
"-v, --version                          Print version and copyright "
"information\n"
msgstr ""
"-v, --version                          Toon versie en auteursrecht "
"informatie\n"

#: option.c:336
msgid "-d <delim>, --delimiters=<delim>       Specifiy delimiters\n"
msgstr ""
"-d <delim>, --delimiters=<delim>       Specificeer afbakeningskarakters\n"

#: option.c:337
msgid ""
"-P, --punctuation                      Use punctuation characters as "
"delimiters\n"
msgstr ""
"-P, --punctuation                      Neem leestekens als "
"afbakeningskarakters\n"

#: option.c:338
msgid "-W <ws>, --white-space=<ws>            Specify whitespace characters\n"
msgstr ""
"-W <ws>, --white-space=<ws>            Specificeer witruimte karakters\n"

#: option.c:341
msgid "-1, --no-deleted                       Do not print deleted words\n"
msgstr "-1, --no-deleted                       Toon verwijderde woorden niet\n"

#: option.c:342
msgid "-2, --no-inserted                      Do not print inserted words\n"
msgstr "-2, --no-inserted                      Toon ingevoegde woorden niet\n"

#: option.c:343
msgid "-3, --no-common                        Do not print common words\n"
msgstr "-3, --no-common                        Toon gelijke woorden niet\n"

#: option.c:344
msgid "-L[<width>], --line-numbers[<width>]   Prepend line numbers\n"
msgstr "-L[<width>], --line-numbers[<width>]   Toon regelnummers\n"

#: option.c:345
msgid "-C<num>, --context=<num>               Show <num> lines of context\n"
msgstr "-C<num>, --context=<num>               Toon <num> regels context\n"

#: option.c:346
msgid "-s, --statistics                       Print statistics when done\n"
msgstr "-s, --statistics                       Toon statistieken\n"

#: option.c:349
msgid "-i, --ignore-case                      Ignore differences in case\n"
msgstr ""
"-i, --ignore-case                      Negeer hoofd/kleine letter verschil\n"

#: option.c:350
msgid "-I, --ignore-formatting                Ignore formatting differences\n"
msgstr "-I, --ignore-formatting                Negeer verschillen in opmaak\n"

#: option.c:351
msgid ""
"-D <option>, --diff-option=<option>    Add <option> to the diff command-"
"line\n"
msgstr ""
"-D <option>, --diff-option=<option>    Voeg <option> toe aan de diff\n"
"                                       commando-regel\n"

#: option.c:355
msgid "-c[<spec>], --color[=<spec>]           Color mode\n"
msgstr "-c[<spec>], --color[=<spec>]           Kleurenmodus\n"

#: option.c:356
msgid ""
"-l, --less-mode                        As -p but also overstrike whitespace\n"
msgstr ""
"-l, --less-mode                        Als -p met doorhalen van witruimte\n"

#: option.c:357
msgid "-p, --printer                          Use overstriking and bold text\n"
msgstr ""
"-p, --printer                          Gebruik doorhalen en dikgedrukte "
"tekst\n"

#: option.c:358
msgid ""
"-w <string>, --start-delete=<string>   String to mark begin of deleted text\n"
msgstr ""
"-w <string>, --start-delete=<string>   Tekenreeks om het begin van "
"verwijderde\n"
"                                       tekst te markeren\n"

#: option.c:359
msgid ""
"-x <string>, --stop-delete=<string>    String to mark end of deleted text\n"
msgstr ""
"-x <string>, --stop-delete=<string>    Tekenreeks om het einde van "
"verwijderde\n"
"                                       tekst te markeren\n"

#: option.c:360
msgid ""
"-y <string>, --start-insert=<string>   String to mark begin of inserted "
"text\n"
msgstr ""
"-y <string>, --start-insert=<string>   Tekenreeks om het begin van "
"toegevoegde\n"
"                                       tekst te markeren\n"

#: option.c:361
msgid ""
"-z <string>, --stop-insert=<string>    String to mark end of inserted text\n"
msgstr ""
"-z <string>, --stop-insert=<string>    Tekenreeks om het einde van "
"toegevoegde\n"
"                                       tekst te markeren\n"

#: option.c:417
#, c-format
msgid "Usage: dwdiff [OPTIONS] <OLD FILE> <NEW FILE>\n"
msgstr "Gebruik: dwdiff [OPTIES] <OUD BESTAND> <NIEUW BESTAND>\n"

