#: option.c:458
#, c-format
msgid "Option %.*s is only supported for UTF-8 mode\n"
msgstr "De optie %.*s wordt alleen ondersteund in UTF-8 mode\n"

#: option.c:461
#, c-format
msgid "Support for option %.*s is not compiled into this version of dwdiff\n"
msgstr ""
"Ondersteuning voor de optie %.*s is niet in deze versie van dwdiff "
"meegecompileerd\n"

#: option.c:468 option.c:479 option.c:498
#, c-format
msgid "Option %.*s is not supported\n"
msgstr "Optie %.*s is niet ondersteund\n"

#: option.c:507
msgid "Can't read both files from standard input\n"
msgstr "Kan niet beide bestanden lezen uit de standard invoer\n"

#: option.c:511 option.c:525 option.c:613
msgid "Too many files to compare\n"
msgstr "Te veel bestanden om te vergelijken\n"

#. TRANSLATORS:
#. "Name" and "Description" are table headings for the color name list.
#. Make sure you keep the alignment of the headings over the text.
#: option.c:544
msgid "Name            Description\n"
msgstr "Naam            Omschrijving\n"

#: option.c:554
#, c-format
msgid "Invalid color specification %s\n"
msgstr "Ongeldige kleur specificatie %s\n"

#: option.c:569 option.c:582
#, c-format
msgid "Invalid color %s\n"
msgstr "Ongeldige kleur %s\n"

#: option.c:603
#, c-format
msgid "Option %.*s does not exist\n"
msgstr "Optie %.*s bestaat niet\n"

#: option.c:618
msgid "Need two files to compare\n"
msgstr "Er zijn twee bestanden nodig om te vergelijken\n"

#: optionMacros.h:50
#, c-format
msgid "Option %.*s has zero length argument\n"
msgstr "Optie %.*s heeft een argument met lengte nul\n"

#: optionMacros.h:89
#, c-format
msgid "Option %.*s does not take an argument\n"
msgstr "Optie %.*s heeft geen argument\n"

#: optionMacros.h:94
#, c-format
msgid "Option %.*s requires an argument\n"
msgstr "Optie %.*s heeft een argument nodig\n"

#: util.c:38
#, c-format
msgid "Could not allocate memory: %s\n"
msgstr "Kon geen geheugen verkrijgen: %s\n"

#: util.c:38
msgid "Out of memory"
