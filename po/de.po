#: option.c:85
#, c-format
msgid "Single backslash at end of %s argument\n"
msgstr "Einfacher backslash am Ende des %s Argumentes\n"

#. TRANSLATORS:
#. The %s argument is a long option name without preceding dashes.
#: option.c:134 option.c:141
#, c-format
msgid "Invalid hexadecimal escape sequence in %s argument\n"
msgstr "Ungültige hexadezimale Escape-Sequenz im %s Argument\n"

#. TRANSLATORS:
#. The %c argument will be either 'u' or 'U'. The %s argument is a long
#. option name without preceding dashes.
#: option.c:169 option.c:175
#, c-format
msgid "Too short \\%c escape in %s argument\n"
msgstr "Zu kurze \\%c Escapesequenz im Argument %s\n"

#. TRANSLATORS:
#. The %s argument is a long option name without preceding dashes.
#: option.c:186
#, c-format
msgid "\\U escape out of range in %s argument\n"
msgstr "\\U Escapesequenz ist außerhalb des zulässigen Bereichs im Argument %s\n"

#. TRANSLATORS:
#. The %s argument is a long option name without preceding dashes.
#: option.c:191
#, c-format
msgid "\\%c escapes for surrogate codepoints are not allowed in %s argument\n"
msgstr "\\%c Escapes für Surrogatkodepunkte sind im %s Argument nicht erlaubt\n"

#: option.c:307 option.c:315 option.c:326
msgid "Whitespace and delimiter sets overlap\n"
msgstr "Whitespace und Begrenzerspezifikation überlappen\n"

#: option.c:332
msgid "-h, --help                             Print this help message\n"
msgstr "-h, --help                             Diese Hilfeseite ausgeben\n"

#: option.c:333
msgid ""
"-v, --version                          Print version and copyright "
"information\n"
msgstr ""
"-v, --version                          Versions- und Copyrightinformationen\n"
"                                       ausgeben\n"

#: option.c:336
msgid "-d <delim>, --delimiters=<delim>       Specifiy delimiters\n"
msgstr "-d <delim>, --delimiters=<delim>       Begrenzer spezifizieren\n"

#: option.c:337
msgid ""
"-P, --punctuation                      Use punctuation characters as "
"delimiters\n"
msgstr 
"-P, --punctuation                      Nutze Zeichensetzungszeichen als"
"Begrenzer\n"

#: option.c:338
msgid 
"-W <ws>, --white-space=<ws>            Specify whitespace characters\n"
msgstr ""
"-W <ws>, --white-space=<ws>            Character für Whitespace "
"spezifizieren\n"

#: option.c:341
msgid "-1, --no-deleted                       Do not print deleted words\n"
msgstr ""
"-1, --no-deleted                       Gelöschte Wörter nicht ausgeben\n"

#: option.c:342
msgid "-2, --no-inserted                      Do not print inserted words\n"
msgstr ""
"-2, --no-inserted                      Eingefügte Wörter nicht ausgeben\n"

#: option.c:343
msgid "-3, --no-common                        Do not print common words\n"
msgstr ""
"-3, --no-common                        Gemeinsame Wörter nicht ausgeben\n"

#: option.c:344
msgid "-L[<width>], --line-numbers[<width>]   Prepend line numbers\n"
msgstr ""
"-L[<width>], --line-numbers[<width>]   Voranstellen von Zeilennummern\n"

#: option.c:345
msgid "-C<num>, --context=<num>               Show <num> lines of context\n"
msgstr "-C<num>, --context=<num>               Zeige <num> Zeilen Kontext\n"

#: option.c:346
msgid "-s, --statistics                       Print statistics when done\n"
msgstr "-s, --statistics                       Statistiken ausgeben\n"

#: option.c:349
msgid "-i, --ignore-case                      Ignore differences in case\n"
msgstr ""
"-i, --ignore-case                      Groß- und Kleinschreibung ignorieren\n"

#: option.c:350
msgid "-I, --ignore-formatting                Ignore formatting differences\n"
msgstr "-I, --ignore-formatting                Ignoriere Formatierungsunterschiede\n"

#: option.c:351
msgid ""
"-D <option>, --diff-option=<option>    Add <option> to the diff command-"
"line\n"
msgstr ""
"-D <option>, --diff-option=<option>    Füge <option> zu den diff\n"
"                                       Kommando-Regeln hinzu\n"

#: option.c:355
msgid "-c[<spec>], --color[=<spec>]           Color mode\n"
msgstr "-c[<spec>], --color[=<spec>]           Farbmodus\n"

#: option.c:356
msgid ""
"-l, --less-mode                        As -p but also overstrike whitespace\n"
msgstr ""
"-l, --less-mode                        Wie -p, mit durchgestrichenen "
"Whitespace\n"

#: option.c:357
msgid "-p, --printer                          Use overstriking and bold text\n"
msgstr ""
"-p, --printer                          Gestrichenen und fetten Text nutzen\n"

#: option.c:358
msgid ""
"-w <string>, --start-delete=<string>   String to mark begin of deleted text\n"
msgstr ""
"-w <string>, --start-delete=<string>   Zeichenkette um den Beginn von "
"gelöschtem\n"
"                                       Text zu markieren\n"

#: option.c:359
msgid ""
"-x <string>, --stop-delete=<string>    String to mark end of deleted text\n"
msgstr ""
"-x <string>, --stop-delete=<string>    Zeichenkette um das Ende von "
"gelöschtem\n"
"                                       Text zu markieren\n"

#: option.c:360
msgid ""
"-y <string>, --start-insert=<string>   String to mark begin of inserted "
"text\n"
msgstr ""
"-y <string>, --start-insert=<string>   Zeichenkette um den Beginn von "
"eigefügtem\n"
"                                       Text zu markieren\n"

#: option.c:361
msgid ""
"-z <string>, --stop-insert=<string>    String to mark end of inserted text\n"
msgstr ""
"-z <string>, --stop-insert=<string>    Zeichenkette um das Ende von "
"eigefügtem\n"
"                                       Text zu markieren\n"

#: option.c:417
#, c-format
msgid "Usage: dwdiff [OPTIONS] <OLD FILE> <NEW FILE>\n"
msgstr "Gebrauch: dwdiff [OPTIONEN] <ALTE DATEI> <NEU DATEI>\n"

